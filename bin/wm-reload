#!/usr/bin/env bash

for service in yabai spacebar seanponeil/formulae/skhd; do
  PID=$(launchctl list | grep "${service}")
  if [[ -n "${PID}" ]]; then
    # Kickstarting active Homebrew services is faster
    # than a full restart
    launchctl kickstart -k "gui/501/homebrew.mxcl.${service}"
  else
    brew services start "${service}"
  fi
done
